class Solution:  
    def largeOddNum(self, s: str) -> str:
        last_index = -1

        for i in range(len(s)-1, 0, -1):
            if int(s[i]) % 2 == 1:
                last_index = i
                break

        if last_index == -1:
            return ''

        res = s[:last_index + 1]
        res = res.lstrip('0')

        return res
